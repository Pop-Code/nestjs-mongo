{
    "name": "nestjs-mongo",
    "version": "0.13.0",
    "description": "A NestJS module that provide a simple mongodb orm like",
    "keywords": [
        "nestjs",
        "module",
        "database",
        "mongodb",
        "mongo"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": "git@github.com:Pop-Code/nestjs-mongo.git",
    "author": "Rmannn <alex.hermann@pop-code.com>",
    "license": "MIT",
    "peerDependencies": {
        "@nestjs/common": "^8",
        "@nestjs/core": "^8",
        "reflect-metadata": "^0.1.13",
        "class-validator": "^0.13.1"
    },
    "dependencies": {
        "class-transformer": "0.4.0",
        "cls-hooked": "4.2.2",
        "debug": "4.3.2",
        "global": "4.4.0",
        "lodash": "4.17.21",
        "mongodb": "4.0.1",
        "slugify": "1.6.0",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@nestjs/common": "8.0.5",
        "@nestjs/core": "8.0.5",
        "@nestjs/testing": "8.0.5",
        "@nestjs/platform-express": "8.0.5",
        "@types/cls-hooked": "4.3.3",
        "@types/debug": "4.1.7",
        "@types/jest": "26.0.24",
        "@types/lodash": "4.14.172",
        "@types/supertest": "2.0.11",
        "@typescript-eslint/eslint-plugin": "4.29.0",
        "@typescript-eslint/parser": "4.29.0",
        "@types/node": "16.4.10",
        "class-validator": "0.13.1",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-config-standard-with-typescript": "20.0.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "5.1.0",
        "eslint-plugin-standard": "5.0.0",
        "jest": "27.0.6",
        "prettier": "2.3.2",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.3.0",
        "supertest": "6.1.4",
        "ts-jest": "27.0.4",
        "ts-node": "10.1.0",
        "tsconfig-paths": "3.10.1",
        "typedoc": "0.21.5",
        "typescript": "4.3.5"
    },
    "scripts": {
        "build": "rm -Rf dist && tsc -b tsconfig.build.json",
        "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write && git status",
        "lint": "eslint .",
        "doc": "rm -Rf ./docs && typedoc ./src && touch ./docs/.nojekyll",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".spec.(t|j)s$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.ts",
            "!index.ts",
            "!test/**",
            "!dist/**"
        ],
        "coverageDirectory": "../coverage"
    },
    "engines": {
        "node": ">=0.12"
    }
}
