{
    "name": "nestjs-mongo",
    "version": "0.13.2",
    "description": "A NestJS module that provide a simple mongodb orm like",
    "keywords": [
        "nestjs",
        "module",
        "database",
        "mongodb",
        "mongo"
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": "git@github.com:Pop-Code/nestjs-mongo.git",
    "author": "Rmannn <alex.hermann@pop-code.com>",
    "license": "MIT",
    "peerDependencies": {
        "@nestjs/common": "^8",
        "@nestjs/core": "^8",
        "reflect-metadata": "^0.1.13",
        "class-validator": "^0.13.1"
    },
    "dependencies": {
        "class-transformer": "0.5.1",
        "cls-hooked": "4.2.2",
        "debug": "4.3.3",
        "global": "4.4.0",
        "lodash": "4.17.21",
        "mongodb": "4.3.1",
        "slugify": "1.6.5",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@nestjs/common": "8.2.6",
        "@nestjs/core": "8.2.6",
        "@nestjs/testing": "8.2.6",
        "@nestjs/platform-express": "8.2.6",
        "@types/cls-hooked": "4.3.3",
        "@types/debug": "4.1.7",
        "@types/jest": "27.4.0",
        "@types/lodash": "4.14.178",
        "@types/supertest": "2.0.11",
        "@typescript-eslint/eslint-plugin": "5.10.2",
        "@typescript-eslint/parser": "^4.0.1",
        "@types/node": "17.0.27",
        "class-validator": "0.13.2",
        "eslint": "^7.12.1",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "^5.0.0",
        "eslint-plugin-standard": "5.0.0",
        "eslint-config-standard-with-typescript": "^21.0.1",
        "jest": "27.4.7",
        "prettier": "2.5.1",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.5.2",
        "supertest": "6.2.2",
        "ts-jest": "27.1.3",
        "ts-node": "10.4.0",
        "tsconfig-paths": "3.12.0",
        "typedoc": "0.22.11",
        "typescript": "4.5.5"
    },
    "scripts": {
        "build": "rm -Rf dist && tsc -b tsconfig.build.json",
        "format": "prettier \"**/*.ts\" --ignore-path ./.prettierignore --write && git status",
        "lint": "eslint .",
        "doc": "rm -Rf ./docs && typedoc ./src && touch ./docs/.nojekyll",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".spec.(t|j)s$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.ts",
            "!index.ts",
            "!test/**",
            "!dist/**"
        ],
        "coverageDirectory": "../coverage"
    },
    "engines": {
        "node": ">=0.12"
    }
}
